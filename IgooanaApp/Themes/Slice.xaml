<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:charts="clr-namespace:IgooanaApp.Charts"
    >
  <Style TargetType="charts:Slice">
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="charts:Slice">
          <Path x:Name="PART_SliceVisual" Stroke="White" StrokeThickness="2">
          </Path>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
</ResourceDictionary>